pipeline {
    agent none

    stages {
        stage('Start') {
            steps {
                echo 'Starting the pipeline...'
            }
        }
        stage('Build') {
            steps {
                echo 'Building the project...'
            }
        }
        stage('Test') {
            steps {
                echo 'Running tests...'
            }
        }
        stage('Package') {
            steps {
                echo 'Packaging the project...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying the application...'
            }
        }
        stage('End') {
            steps {
                echo 'Pipeline finished.'
            }
        }
    }
}
